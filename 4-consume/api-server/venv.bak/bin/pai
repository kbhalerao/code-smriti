#!/Users/kaustubh/Documents/code/code-smriti/4-consume/api-server/venv/bin/python3
# -*- coding: utf-8 -*-
import sys
from pydantic_ai._cli import cli_exit
if __name__ == "__main__":
    if sys.argv[0].endswith("-script.pyw"):
        sys.argv[0] = sys.argv[0][:-11]
    elif sys.argv[0].endswith(".exe"):
        sys.argv[0] = sys.argv[0][:-4]
    sys.exit(cli_exit())
